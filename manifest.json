{
  "manifest_version": 2,
  "name": "MyHomeTab",
  "version": "1.0",
  "description": "A customizable new tab page.",
  "icons": {
    "16": "images/my_home_tab_icon16.png",
    "48": "images/my_home_tab_icon48.png",
    "128": "images/my_home_tab_icon128.png"
  },
  "incognito": "spanning",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "permissions": [
    "geolocation",
    "storage"
  ]
}
